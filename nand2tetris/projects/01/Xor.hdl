/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
	Not (in=a, out=notA);
	Not (in=b, out=notB);
	And (a=notA, b=b, out=temp1);
	And (a=a, b=notB, out=temp2);
	Or (a=temp1, b=temp2, out=out);
}


